 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetDisplayNameServlet</servlet-name>
  	<display-name>GetDisplayNameServlet</display-name>
  	<description></description>
  	<servlet-class>com.stevenckwong.ACWebServicesIntegration.GetDisplayNameServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetTestCaseServlet</servlet-name>
  	<display-name>GetTestCaseServlet</display-name>
  	<description></description>
  	<servlet-class>com.stevenckwong.ACWebServicesIntegration.GetTestCaseServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetTimeboxServlet</servlet-name>
  	<display-name>GetTimeboxServlet</display-name>
  	<description></description>
  	<servlet-class>com.stevenckwong.ACWebServicesIntegration.GetTimeboxServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreateTimeboxServlet</servlet-name>
