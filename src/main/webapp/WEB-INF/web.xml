  	<servlet-class>com.stevenckwong.ACWebServicesIntegration.DeleteTimeboxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetDisplayNameServlet</servlet-name>
  	<url-pattern>/GetDisplayNameServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetTestCaseServlet</servlet-name>
  	<url-pattern>/GetTestCaseServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetTimeboxServlet</servlet-name>
  	<url-pattern>/GetTimeboxServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateTimeboxServlet</servlet-name>
  	<url-pattern>/CreateTimeboxServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteTimeboxServlet</servlet-name>
  	<url-pattern>/DeleteTimeboxServlet</url-pattern>
  </servlet-mapping>
